*  .param modified
*   CPIP_VC1 = -1.472e-4 -> -1.473e-4
*   CPIP_VC2 = -1.257e-5 -> -1.258e-5
*   TC1 = 1.053e-5 -> 1.507e-5
*   TC2 = 2.839e-7 -> 1.648e-7
*
* 05/16/2017 : Ver.0.2
*  Converted Model file to be sigma based and enable monte carlo.
*  Created subcircuit for capacitor so it can vary for every instance during Monte Carlo.
*  Had to put the Voltage Coefficient in the calculation of capacitance because HSPICE & TSPICE
*    use the following formula C = Cnom*(1 + VC1*abs(V) + VC2*V*V).
*
* 04/04/2017 : Ver.0.0
* First release 
*
*************************************************************************

.LIB tt
  .PARAM Cap_Sigma = 0
	.LIB CPIP.lib cap
.ENDL tt

.LIB ff
	.PARAM Cap_Sigma = -3
	.LIB CPIP.lib cap
.ENDL ff

.lib ss
	.PARAM Cap_Sigma = 3
	.LIB CPIP.lib cap
.ENDL ss

.lib sigma
	.LIB CPIP.lib cap
.ENDL sigma

.lib mc
	.PARAM Cap_Sigma = AGAUSS(0,1,1)
	.LIB CPIP.lib cap
.ENDL mc

.LIB cap     $comment out by tsuchiya
.SUBCKT CPIP Pos Neg L=10u W=10u m=1
.PARAM CPIP_CoxMean = 1.72e-3
.PARAM CPIP_CoxStDev = 'CPIP_CoxMean * 0.1/3'
.PARAM CPIP_Cox = 'CPIP_CoxMean + Cap_Sigma*(CPIP_CoxStDev)'
.PARAM CPIP_Csw = 0
.PARAM CPIP_dL = 0
.PARAM CPIP_dW = 0

.PARAM CPIP_Leff = 'L+CPIP_dL'
.PARAM CPIP_Weff = 'W+CPIP_dW'
.PARAM CPIP_Area = 'CPIP_Leff*CPIP_Weff'
.PARAM CPIP_Peri = '2.0*(CPIP_Leff+CPIP_Weff)'
.PARAM CPIP_Cnom = 'CPIP_Cox*CPIP_Area + CPIP_Csw*CPIP_Peri'
.PARAM CPIP_VC1 = -1.473e-4
.PARAM CPIP_VC2 = -1.258e-5

Cmain Pos Neg CPIP C='CPIP_Cnom*(1 + CPIP_VC1*V(Pos,Neg) + CPIP_VC2*V(Pos,Neg)*V(Pos,Neg))'
*Cmain Pos Neg CPIP C='CPIP_Cnom'

.MODEL CPIP C
+ TC1 = 1.507e-5
+ TC2 = 1.648e-7
*+ VC1 = 'CPIP_VC1'
*+ VC2 = 'CPIP_VC2'
+ TNOM = 27
.ENDS CPIP
.ENDL cap  $comment out by tsuchiya

